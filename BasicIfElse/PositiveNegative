class PositiveNegative{
    public static void public static void main(String[] args) {
        int num=10;
        if(num==0){
            System.out.println(num + " is Zero");
        }else if(num>0){
            System.out.println(num + " is Positive");
        }else{
            System.out.println(num + "is Negative");
        }
        
    }
}