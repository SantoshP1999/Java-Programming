class GreaterThan10{
    public static void main(String []args) {
        int num=20;
        if(num<0){
            System.out.println(num + " is less than 0");
        }else if(num>10){
            System.out.println(num +" is Greater than 10");
        }else{
            System.out.println(num + " is less than 10");
        }
        
    }
}